@model TMS.Models.Tenant

@{
    ViewBag.Title = "Delete Confirmation";
}

<h2>Are you sure you want to delete this Tenant?</h2>
<h2>@Model.TenantName</h2>

<form action="/tenant/delete/@Model.TenantId" method="POST">
    <input type="submit" value="Confirm Delete" class="btn btn-success" />
    <a class="btn btn-danger" href="/tenant/view/@Model.TenantId">Cancel</a>

</form>